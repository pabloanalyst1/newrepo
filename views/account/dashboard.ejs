<h1>Welcome <%= accountData.account_firstname %>!</h1>

<p>You are now logged in.</p>

<ul>
  <li><a href="/account/edit">Update Account Info</a></li>
  <% if (accountData.account_type === 'Admin') { %>
    <li><a href="/inv">Manage Inventory</a></li>
  <% } %>
</ul>

<form action="/account/logout" method="post">
  <button type="submit">Logout</button>
</form>
